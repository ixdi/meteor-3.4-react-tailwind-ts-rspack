{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["imports"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "../docs/",
    "readme": "../README.md",
    "recurse": true
  }
}
